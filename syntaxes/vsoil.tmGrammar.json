{
  "scopeName": "source.vsoil",
  "patterns": [
  {
    "match": "^[a-zA-z]{7} - .*\\.txt$",
    "name": "vsoil.filetype.txt"
  },
  {
    "match": "^[a-zA-z]{7} - .*\\.(c|cpp|h|hpp|cc|cxx|hh|hxx|py|sh|bat|ts|js|java|rb|go|html|css)$",
    "name": "vsoil.filetype.code"
  },
  {
    "match": "^[a-zA-z]{7} - .*$",
    "name": "vsoil.filetype.generic"
  },
  {
    "match": "^[a-zA-z]{7} / .*$",
    "name": "vsoil.dirtype.dir"
  },
  {
    "match": "\\.\\./",
    "name": "vsoil.dirtype.dir"
  }
],
  "repository": {

  }
}