{
  "scopeName": "source.voil",
  "patterns": [
  {
    "match": "\\A(.*/)(.*?)$",
    "captures": {
      "1": {
        "name": "voil.currentDirectory"
      },
      "2": {
        "name": "voil.currentDirectoryGlob"
      }
    }
  },
  {
    "match": "^\\/[a-zA-Z]{20} - .*\\.txt$",
    "name": "voil.filetype.txt"
  },
  {
    "match": "^\\/[a-zA-Z]{20} - .*\\.(c|cpp|h|hpp|cc|cxx|hh|hxx|py|sh|bat|ts|js|java|rb|go|html|css)$",
    "name": "voil.filetype.code"
  },
  {
    "match": "^\\/[a-zA-Z]{20} - .*$",
    "name": "voil.filetype.generic"
  },
  {
    "match": "^\\/[a-zA-Z]{20} / .*$",
    "name": "voil.dirtype.dir"
  },
  {
    "match": "\\.\\./",
    "name": "voil.dirtype.dir"
  },
  {
    "match": "^[\\s\\S]*$",
    "name": "voil.unlisted"
  }
],
  "repository": {

  }
}