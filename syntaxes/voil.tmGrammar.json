{
  "scopeName": "source.voil",
  "patterns": [
  {
    "match": "\\A(.*/)(.*?)$",
    "captures": {
      "1": {
        "name": "string.other.link"
      },
      "2": {
        "name": "entity.name.function"
      }
    }
  },
  {
    "match": "^\\/[a-zA-Z]{20} -.*\\.txt$",
    "name": "string.quoted.double"
  },
  {
    "match": "^\\/[a-zA-Z]{20} -.*\\.(c|cpp|h|hpp|cc|cxx|hh|hxx|py|sh|bat|ts|js|java|rb|go|html|css)$",
    "name": "entity.name.type"
  },
  {
    "match": "^\\/[a-zA-Z]{20} -.*$",
    "name": "variable.other.readwrite"
  },
  {
    "match": "^\\/[a-zA-Z]{20} /.*$",
    "name": "keyword.control"
  },
  {
    "match": "\\.\\./",
    "name": "keyword.control"
  },
  {
    "match": "^[\\s\\S]*$",
    "name": "comment.line"
  }
],
  "repository": {

  }
}