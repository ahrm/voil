{
  "scopeName": "source.voil",
  "patterns": [
  {
    "match": "^\\/[a-zA-z]{7} - .*\\.txt$",
    "name": "voil.filetype.txt"
  },
  {
    "match": "^\\/[a-zA-z]{7} - .*\\.(c|cpp|h|hpp|cc|cxx|hh|hxx|py|sh|bat|ts|js|java|rb|go|html|css)$",
    "name": "voil.filetype.code"
  },
  {
    "match": "^\\/[a-zA-z]{7} - .*$",
    "name": "voil.filetype.generic"
  },
  {
    "match": "^\\/[a-zA-z]{7} / .*$",
    "name": "voil.dirtype.dir"
  },
  {
    "match": "\\.\\./",
    "name": "voil.dirtype.dir"
  },
  {
    "match": "^[\\s\\S]*$",
    "name": "voil.unlisted"
  }
],
  "repository": {

  }
}